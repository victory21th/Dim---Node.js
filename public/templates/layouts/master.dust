            <ul ng-controller="NavigationController" class="nav navbar-nav">
              <li ng-repeat="item in menu" ng-if="!(item.hideOnHorizontal||item.separator)" ng-class="{ hasChild: (item.children!==undefined), active: item.selected, open: (item.children!==undefined) && item.open, 'nav-separator': item.separator==true }" ng-include="'app/views/templates/nav_renderer_horizontal.html'"
              ></li>
            </ul>
        </div>
      </nav>

