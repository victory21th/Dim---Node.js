            <div class="sidebar">
            <!--
              <div class="widget" ng-cloak>
                    <div class="widget-body">
                        <div class="userinfo">
                            <div class="avatar">
                                <img masked-image data-ng-src="{{ auth.profile.picture }}" class="img-responsive img-circle">
                            </div>
                            <div class="info">
